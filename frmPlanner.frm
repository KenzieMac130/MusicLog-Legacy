Private Sub txtBlock_Change()
    'calls ADD feature after text block entry changes
On Error GoTo HandleErrors

    If txtBlock = "" Then
        cmdClearBlock.Caption = "Set"
    End If
       
    If Not IsNumeric(txtBlock) And txtBlock <> "" Then
        MsgBox "You have entered the non-numeric character or character group:   " & txtBlock & vbCrLf & vbCrLf & _
        "Enter an estimate of the average number of minutes of music planned for the hour." & vbCrLf & _
        "56 minutes of music in an hour is the default setting" & vbCrLf & vbCrLf & _
        "If seconds are included, they should be entered as the decimal part of the minute." & vbCrLf & _
        "Example: Fifty-six minutes and thirty seconds would be entered as 56.5." & vbCrLf & vbCrLf & _
        "Entry cannot exceed 120 minutes (2 hours).", vbOKOnly, "Planned Music Time Entry Error"
        
        giNotNumeric = 1
        txtBlock = "0"
        
        txtBlock.SelStart = 0 'begin selection at start
        txtBlock.SelLength = Len(txtBlock)
    
        Label5.ToolTipText = " Enter the number of minutes of music planned for the hour. "
        txtBlock.SetFocus
        Exit Sub
    End If
    
    If Val(txtBlock) > 120.01 Then
        MsgBox "Planned Music Time entry cannot exceed 120 minutes (two hours)", vbOKOnly, "Entry Exceeds 120 Minute Limit"
        txtBlock = "56.0"
        pSetFocus
        Exit Sub
    End If
    
    If txtBlock.Text = "56.0" Then
        txtBlock.ForeColor = &H80000008  'black
         txtBlock.Appearance = 1
        txtBlock.BorderStyle = 1
    Else
        txtBlock.ForeColor = &HC00000 'blue   '&H80& ' rust
        txtBlock.Appearance = 0
        txtBlock.BorderStyle = 1
    End If
    
    Dim iBlock As Integer

    If IsNumeric(txtBlock) Then 'Or txtBlock = "" Then
        iBlock = Val(txtBlock)
        giNotNumeric = 0
        Call pAdd
   End If
    
'----------------------
HandleErrors:
    
    lblRemain2 = txtBlock
    
    If lblRemain2 <> "" Then
        Label7.Visible = True
        lblForthehour.Visible = True
        lblExceeds.Visible = True
    
        If Val(txtBlock) < 60.01 Then
            lblForthehour.Caption = "Music Time for the hour"
        Else
            lblForthehour.Caption = "Music Time"
        End If
    
    Else
        Label7.Visible = False
        lblForthehour.Visible = False
        lblExceeds.Visible = False
    End If

 End Sub

