Private Sub lblListCount_Change()
    
    If lblListCount = "0" Then
        lblListCount = ""
        lblLineCount = ""
    ElseIf lblListCount <> "" Or lblListCount <> "0" Then
        lblLineCount = "Line Count:"
    End If

    miListCount = Val(lblListCount)
 
    If mnuPrintArial.Checked = False Then
    
       If miFontSize = 0 And miListCount > 49 Then  'ARIAL smaller
          lblListCount.ToolTipText = " Arial normal type, 59 lines per page. Large type, 53 lines."
          lblLineCount.ToolTipText = " Arial normal type, 59 lines per page. Large type, 53 lines."
          
       ElseIf miFontSize = 1 And miListCount > 43 Then 'larger
          lblListCount.ToolTipText = " Arial large type 53, lines per page. Normal type, 59 lines."
          lblLineCount.ToolTipText = " Arial large type 53, lines per page. Normal type, 59 lines."
       Else
          lblListCount.ToolTipText = ""
          lblLineCount.ToolTipText = ""
       End If
       
    ElseIf mnuPrintArial.Checked = True Then
    
      If miFontSize = 0 And miListCount > 43 Then 'TIMES NEW ROMAN, smaller
          lblListCount.ToolTipText = " Times New Roman normal type, 53 lines per page. Large type, 48 lines."
          lblLineCount.ToolTipText = " Times New Roman normal type, 53 lines per page. Large type, 48 lines."
          
       ElseIf miFontSize = 1 And miListCount > 38 Then 'larger
          lblListCount.ToolTipText = " Times New Roman large type 48, lines per page. Normal type, 53 lines."
          lblLineCount.ToolTipText = " Times New Roman large type 48, lines per page. Normal type, 53 lines."
       Else
          lblListCount.ToolTipText = ""
          lblLineCount.ToolTipText = ""
       End If
    
    End If
 '--------------------------
 'Arial Small
 
    If miFontSize = 0 And mnuPrintArial.Checked = True Then
    
        If miListCount >= 55 And miListCount <= 59 Then 'warning approaching 2 page printout, 4 more lines
            lblLineCount.Caption = "Line Count:"
        
        ElseIf miListCount > 59 And miListCount <= 118 Then '58 is max line count for 1 printed page
            lblLineCount.Caption = "(2 pages) Line Count:"
        
        ElseIf miListCount > 118 And miListCount <= 178 Then
            lblLineCount.Caption = "(3 pages) Line Count:"
         
        ElseIf miListCount > 178 And miListCount <= 236 Then 'four pages
            lblLineCount.Caption = "(4 pages) Line Count:"
            
         ElseIf miListCount > 236 Then 'more than four pages
            lblLineCount.Caption = "(4+ pages) Line Count:"
        
        ElseIf miListCount < 55 Then
            lblLineCount.Caption = "Line Count:"
            
        Else
            lblLineCount.Caption = "Line Count:"
        End If
    End If
    
'ARIAL large

    If miFontSize = 1 And mnuPrintArial.Checked = True Then
   
        If miListCount >= 50 And miListCount <= 53 Then 'warning approaching 2 page printout
            lblLineCount.Caption = "Line Count:"
        
        ElseIf miListCount > 53 And miListCount <= 106 Then '52 is max line count for 1 printed page
            lblLineCount.Caption = "(2 pages) Line Count:"
        
        ElseIf miListCount > 106 And miListCount <= 159 Then 'three
            lblLineCount.Caption = "(3 pages) Line Count:"
            
        ElseIf miListCount > 160 And miListCount <= 213 Then 'four pages
            lblLineCount.Caption = "(4 pages) Line Count:"
            
        ElseIf miListCount > 213 Then  'more than four pages
            lblLineCount.Caption = "(4+ pages) Line Count:"
        
        ElseIf miListCount < 50 Then
            lblLineCount.Caption = "Line Count:"
            
        Else
            lblLineCount.Caption = "Line Count:"
        End If
    End If
    
'=========================

'Times New Roman small
 
    If miFontSize = 0 And mnuPrintArial.Checked = False Then
    
        If miListCount >= 50 And miListCount <= 53 Then 'warning approaching 2 page printout
            lblLineCount.Caption = "Line Count:"
        
        ElseIf miListCount > 53 And miListCount <= 108 Then '58 is max line count for 1 printed page
