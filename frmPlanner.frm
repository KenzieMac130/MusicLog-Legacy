      .FilterIndex = 1
      .InitDir = CurDir$
      .DialogTitle = "Open Program List..."
      .Flags = cdlOFNFileMustExist + cdlOFNPathMustExist + cdlOFNReadOnly
      .ShowOpen
      WhichFile = CommonDialog1.FileName
    End With
    
    iSave = 1 'saves list if it is opened for editing
    lstList.Clear
    lstListNotes.Clear
    lstListSpace.Clear
    lstListNoteSpace.Clear
    lstVoiceTracks.Clear
    lstVoiceTracks1.Clear
