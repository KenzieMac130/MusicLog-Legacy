    cDirectory = CurDir
   ' ChDir miDirectory
    ChDir "MusicLog Archives"
    lblPath = CurDir

On Error GoTo DiaLogError
    Dim WhichFile As String
    With CommonDialog1
      .CancelError = True
     ' .Filter = "MusicLog (*.mlx; *.xml)|* .mlx; *.xml|MusicLog, all versions (*.mlx; *.xml; *.dat)|*.mlx;*.xml;*.dat|"
