    'sets Block Time label readings
    If cRemain >= 0 Then
        lblRemain.ForeColor = &H80& 'rust
        lblExceeds.ForeColor = &H80&
        lblRemain2.ForeColor = &H80&
        Label7.ForeColor = &H80&
