Private Sub cmdInsertVTcue_Click()

    Dim iResponse As Integer
        
    If lstVoiceTracks1.Visible = True Then
    
    If cmdInsertVTcue.Caption = "Insert this line above a highlighted  'md track #'  line: (The following track contains a 3 sec pause between back-annc && intro for spot insert or CD track division)" Then
   
        Dim i As Integer
        Open "ListVT22.dat" For Output As #322 'saves voice-track pad entry for undo
        For i = 0 To lstVoiceTracks1.ListCount - 1
        Print #322, lstVoiceTracks1.List(i)
        Next
        Close #322
    
        If lstVoiceTracks1.ListIndex > -1 And lstVoiceTracks1 <> "" Then

            iResponse = MsgBox("If the highlighted line is an 'md track' line click OK to insert this line above it " _
            & vbCrLf & vbCrLf & "(The following track contains a 3 sec pause between back-annc & intro for spot insert or CD track division)" _
            & vbCrLf & vbCrLf & "Important: For proper placement the highlighted line must be an 'md track #' line.", _
            vbOKCancel, "Important: Highlighted line must be an 'md track #' line")

                If iResponse = vbCancel Then
                    Exit Sub
                End If
                
            lstVoiceTracks1.AddItem "", lstVoiceTracks1.ListIndex - 2
            lstVoiceTracks1.AddItem "   (The following track contains a 3 sec pause between back-annc & intro for spot insert or CD track division)", lstVoiceTracks1.ListIndex - 1
             
            cmdInsertVTcue.Caption = "Cancel Inserted 'Back-Anc Pause Intro' Line"
        Else
        
            iResponse = MsgBox("No line has been highlighted or the highlighted line is blank " _
            & vbCrLf & vbCrLf & "The highlighted line should be an 'md track #' line." _
            & vbCrLf & vbCrLf & "Click a line only once to highlight it. Double-clicking will open an edit box.", _
              vbOKOnly, "No line has been highlighted or the highlighted line is blank")
'
            Exit Sub
        End If

        If msArchiveOpen = 1 Then
            miSavePrint = 0
            optPlannerPage.Enabled = True
        End If
    
         'counts list lines
        If lstVoiceTracks1.Visible = True Then
            lblListCount = lstVoiceTracks1.ListCount
        End If
        pSetFocus

    ElseIf Not cmdInsertVTcue.Caption = "Insert this line above a highlighted  'md track #'  line: (The following track contains a 3 sec pause between back-annc && intro for spot insert or CD track division)" Then

        lstVoiceTracks1.Clear
        Dim sTemp3 As String
        
        Open "ListVT22.dat" For Input As #322
            Do Until EOF(322)
            Line Input #322, sTemp3
            lstVoiceTracks1.AddItem sTemp3
            Loop
        Close #322
        
        cmdInsertVTcue.Caption = "Insert this line above a highlighted  'md track #'  line: (The following track contains a 3 sec pause between back-annc && intro for spot insert or CD track division)"
    End If
End If
    
End Sub


