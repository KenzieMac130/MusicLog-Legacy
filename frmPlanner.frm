        chkLinerNotes.Enabled = False
        chkLinerNotes.Value = 0
        chkVoiceTracks.Enabled = False
        chkVoiceTracks.Value = 0
        chkVoiceTracks1.Enabled = False
        chkVoiceTracks1.Value = 0
    End If
    
    Open CommonDialog1.FileName For Input As #iFileNumber
        Do Until EOF(iFileNumber)
        Line Input #iFileNumber, sTemp
        lstList.AddItem sTemp
        lstListNotes.AddItem sTemp
        lstListSpace.AddItem sTemp
        lstListNoteSpace.AddItem sTemp
