Private Sub txtSpots_Change()

    If Not IsNumeric(txtSpots) And txtSpots <> "" Then
       MsgBox "You have entered a non-numeric character", vbOKOnly, "Entry Error"
        txtSpots.Text = ""
        txtSpots.SetFocus
        Exit Sub
    End If
'----------

    If Val(frmTimeRemain!txtSpotLength) <= 35 Then
    
        If Val(txtSpots) > 10 Then
            MsgBox "You have entered " & txtSpots & " spot/inserts. Ten is the maximum for an hour.", vbOKOnly, "Spot / PSA / Wx Inserts Limited to 10"
            txtSpots.Text = ""
            txtSpots.SetFocus
            Exit Sub
        End If
        
     ElseIf Val(frmTimeRemain!txtSpotLength) > 35 And Val(frmTimeRemain!txtSpotLength) <= 65 Then
     
        If Val(txtSpots) > 8 Then
            MsgBox "You have entered " & txtSpots & " spot/inserts. Eight is the maximum for an hour.", vbOKOnly, "Spot / PSA / Wx Inserts Limited to 8"
            txtSpots.Text = ""
            txtSpots.SetFocus
            Exit Sub
        End If
     
     ElseIf Val(frmTimeRemain!txtSpotLength) > 65 Then
        
        If Val(txtSpots) > 6 Then
            MsgBox "You have entered " & txtSpots & " spot/inserts. Six is the maximum for an hour.", vbOKOnly, "Spot / PSA / Wx Inserts Limited to 6"
            txtSpots.Text = ""
            txtSpots.SetFocus
            Exit Sub
        End If
     
     End If
    
'----------
    If Val(txtSpots) <= 0 Or txtSpots = "" Then
        giPlannerTxtSpot = 0
    ElseIf Val(txtSpots) > 0 Then
        giPlannerTxtSpot = 1
    End If
    
    If Val(txtSpots) >= 2 Then
        txtSpots.ToolTipText = "Double-click to reduce value by half"
        lblSpotLength.ToolTipText = "Double-click to reduce value by half"
    Else
        txtSpots.ToolTipText = ""
        lblSpotLength.ToolTipText = ""
    End If
    
    frmTimeRemain!txtSpotsS = txtSpots
       
    If Val(txtSpots) = 0 Then
        lblSpotLength.Caption = Val(frmTimeRemain!txtSpotLength) & " Sec Spot(s)"
        lblSpotLength.ForeColor = &H80000008  'black
    ElseIf Val(txtSpots) = 1 Then
        lblSpotLength.Caption = Val(frmTimeRemain!txtSpotLength) & " Sec Spot  (" & Val(txtSpots) * Val(frmTimeRemain!txtSpotLength) & " Secs)"
        lblSpotLength.ForeColor = vbRed
    ElseIf Val(txtSpots) > 1 Then
        lblSpotLength.Caption = Val(frmTimeRemain!txtSpotLength) & " Sec Spots  (" & Val(txtSpots) * Val(frmTimeRemain!txtSpotLength) & " Secs)"
        lblSpotLength.ForeColor = vbRed
    End If

End Sub

