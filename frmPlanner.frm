    txtPad6.Visible = False
    txtPad7.Visible = False
    txtPad8.Visible = False
    
    Dim iFileNumber As Integer
    iFileNumber = FreeFile
    Dim sTemp As String
    
    If chkVoiceTracks.Value = 1 Then
        chkVoiceTracks.Value = 0
    End If
    
    If chkVoiceTracks1.Value = 1 Then
        chkVoiceTracks1.Value = 0
    End If
    
    chkSpace.Value = 0
    chkLinerNotes.Value = 0
    
    If txtComposer1 = "" Or txtComposer1 = "?" Or txtComposer2 = "" Then
        chkSpace.Enabled = False
