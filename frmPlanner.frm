Private Sub txtCD1_Change()

    If iLP1 = 0 Then
        If (UCase(txtCD1) = "LP" And Len(txtCD1) = 2) Or (UCase(txtCD1) = " LP" And Len(txtCD1) = 3) _
        Or (UCase(txtCD1) = "'LP" And Len(txtCD1) = 3) Then

                txtCD1 = "LP# "
                txtCD1.SelStart = 4 'begins highlight with 4th position in text
                iLP1 = 1 'limits "LP#" to single occurance
        End If
        
    Else
        If (UCase(txtCD1) = "LP#") Then
        txtCD1 = "LP# "
        txtCD1.SelStart = 4 'begins highlight with 4th position in text
        End If
    End If
    
    If txtComposer1 <> "" And (txtComposer1 <> "?" Or txtComposition1 <> "") And miPreviousLineUp = 0 Then
 
        Select Case UCase(Left(txtCD1.Text, 3))
            Case Is <> "LP#"
                lblCD.Caption = "CD Number "
            
            Case Is = "LP#"
                lblCD.Caption = "LP Number "
        End Select
    End If

    '------------------------------------set txtCD length to remain within 78 characters
    Dim iLength1 As Integer
    iLength1 = (Len(txtComposer1) + Len(txtComposition1) + Len(txtTrack1)) 'max 23 + 35 + 8 = 66  23+40+8 = 71

     If iLength1 <= 54 Then '54 + 22 = 76 (max 78)

        If UCase(Left(txtCD1.Text, 4)) = "LP# " Then 'CD max 22 characters
            txtCD1.MaxLength = 36 - Len(txtCD1) '18, adjusts 4 for "LP#" printed on the line
        Else
            txtCD1.MaxLength = 44 - Len(txtCD1) 'full 22
        End If
        
    Else
        Select Case iLength1
        Case 55
            txtCD1.MaxLength = 42 - Len(txtCD1) '21 max length, found by subtracting 21 from 42 to equal 21
        Case 56                                     'case # + CD length = 76
            txtCD1.MaxLength = 40 - Len(txtCD1) '20
        Case 57
            txtCD1.MaxLength = 38 - Len(txtCD1) '19
        Case 58
            txtCD1.MaxLength = 36 - Len(txtCD1) '18
        Case 59
            txtCD1.MaxLength = 34 - Len(txtCD1) '17
        Case 60
            txtCD1.MaxLength = 32 - Len(txtCD1) '16
        Case 61
            txtCD1.MaxLength = 30 - Len(txtCD1) '15
        Case 62
            txtCD1.MaxLength = 28 - Len(txtCD1) '14
        Case 63
            txtCD1.MaxLength = 26 - Len(txtCD1) '13
        Case 64
            txtCD1.MaxLength = 24 - Len(txtCD1) '12
        Case 65
            txtCD1.MaxLength = 22 - Len(txtCD1) '11
        Case 66
            txtCD1.MaxLength = 20 - Len(txtCD1) '10
        Case 67
            txtCD1.MaxLength = 18 - Len(txtCD1) '9
        Case 68
            txtCD1.MaxLength = 16 - Len(txtCD1) '8
        Case 69
            txtCD1.MaxLength = 14 - Len(txtCD1) '7
        Case Else
            txtCD1.MaxLength = 14 - Len(txtCD1) '7
        End Select
        
    End If
'------------------------
    If txtCD1 <> "" And txtDisc1 = "" And frmPlanner.Visible Then
        txtDisc1 = "1"
    ElseIf txtCD1 = "" Then
        txtDisc1 = ""
    End If

    If mnuLinkTimeRemain.Checked = True And txtComposer1 <> "?" And txtComposer1 <> "" And IsNumeric(txtCD1) Then
        frmTimeRemain!txtCD4 = txtCD1
    End If

    If txtSecond1.Text <> "" Then

        Label11(0).Visible = True
        'Label11(0).Visible = False
        Label11(1).Visible = False
        Label11(2).Visible = False
        Label11(3).Visible = False
        Label11(4).Visible = False
        Label11(5).Visible = False
        Label11(6).Visible = False
        Label11(7).Visible = False
    End If
 '-----------------
    miTotalLength1 = (Len(txtComposer1) + Len(txtComposition1) + Len(txtTrack1) + Len(txtCD1))

    Label11(0) = 78 - miTotalLength1

    If Label11(0) <= -1 Then
        txtCD1.MaxLength = Len(txtCD1)  'max locked
    End If
    
    If mnuLinkTimeRemain.Checked = True Then
        If txtCD1 <> "" Then
            frmTimeRemain!txtCD4 = txtCD1
        Else
            frmTimeRemain!txtCD4 = ""
        End If
    End If
