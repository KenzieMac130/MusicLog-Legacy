    End If
            
    Open "PrintHost.dat" For Output As #403
    Write #403, cboYou, cboProgram
    Close #403
        
    Dim blnItemFound1 As Boolean
    Dim intItemIndex1 As Integer
    
    blnItemFound1 = False
    intItemIndex1 = 0
    
    Do Until blnItemFound1 Or intItemIndex1 = cboYou.ListCount
     
