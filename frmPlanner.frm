Private Sub txtEdit_Change()

'Equivalents: 113 equiv. 106; 144 equiv. 140

'-------------------text color---------

    If Len(txtEdit) <= 113 Then
        lblEditBox.ForeColor = &H80&       'rust
       
    ElseIf Len(txtEdit) > 113 And Len(txtEdit) <= 139 Then
        lblEditBox.ForeColor = &HC00000    'Blue
        
     ElseIf Len(txtEdit) > 139 Then
        lblEditBox.ForeColor = &HC0&       'red
    
    End If
'--------------------counts & caption----------

    If Len(txtEdit) <= 22 Then
        lblEditBox.Caption = "Edit Box"
        
'counts----

    ElseIf Len(txtEdit) > 22 And Len(txtEdit) <= 112 Then
        lblEditBox.Caption = "Remain " & 113 - Len(txtEdit) & vbCrLf & "characters"
        lblExtended.Visible = False
        
 'captions----
        
     ElseIf Len(txtEdit) = 113 Then
        lblEditBox.Caption = "Normal" & vbCrLf & "Max 106"
        lblExtended.Visible = False
       Beep
        
    ElseIf Len(txtEdit) > 113 And Len(txtEdit) <= 143 Then
        lblEditBox.Caption = "EXTENDED" & vbCrLf & "by " & (113 - Len(txtEdit)) * (-1)
         lblExtended.Visible = True
         
    ElseIf Len(txtEdit) = 144 Then
        lblEditBox.Caption = "EXTENDED" & vbCrLf & "Max 140"
        lblExtended.Visible = True
        Beep
        
    ElseIf Len(txtEdit) > 144 Then
        lblEditBox.Caption = "EXCEEDED" & vbCrLf & "Max 140"
        lblExtended.Visible = True
    End If
    
'================

    If txtEdit.Visible = True And Len(txtEdit) > 10 And Len(txtEdit) < 114 Then
        ProgressBar1.Visible = True
        Shape2.Visible = True
        ProgressBar1.Min = 0
        ProgressBar1.Max = 114
        ProgressBar1.Value = Len(txtEdit)
    Else
        ProgressBar1.Value = 0
        ProgressBar1.Visible = False
        Shape2.Visible = False
'        txtCompLineNumber.Caption = ""
'       ' txtCompLineNumber.BackColor = &H8000000F  'button face
    End If
   
End Sub

