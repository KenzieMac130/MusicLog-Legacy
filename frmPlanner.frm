Private Sub txtSignature_DblClick()

    txtSignature.BackColor = &H80000005
    txtSignature.BorderStyle = 1
    txtSignature.Locked = False
        
On Error GoTo HandleErrors

    Dim Signature As String
    Open "Signature.dat" For Input As #27
        Input #27, Signature
    Close #27
    If txtSignature.Text <> Signature Then
        txtSignature.Text = Signature
    End If
  
    txtSignature.SelStart = 0
    txtSignature.SelLength = Len(txtSignature) 'selects all of the characters
    txtSignature.ToolTipText = ""
    
    If txtSignature.Text = "" Then
        txtSignature.Text = frmDefaults!txtSignatureLine.Text
    End If
Exit Sub

HandleErrors:
    txtSignature.Text = "Fine Arts Radio"
     Open "Signature.dat" For Output As #27
        Write #27, "Fine Arts Radio"
    Close #27
End Sub

