Private Sub mnuArchiveDateStamp_CliCk()

On Error GoTo HandleErrors
    Dim iResponse As Integer
    Dim i As Integer
    dDate = Now()
  
    If lstListNotes.ListCount < 1 Then
        
        iResponse = MsgBox("The Date-Stamp follows the last music listing on the lower screen." & vbCrLf & vbCrLf & _
        "The screen is blank or has insufficient data.", vbOKOnly + vbExclamation, "No Data")
        Exit Sub
    End If
   
    If miDateStamp = 2 Then
        MsgBox "A Date-Stamp has already been inserted.", vbOKOnly, "Date-Stamp Inserted"
    Exit Sub
    Else
        iResponse = MsgBox("Insert this Date-Stamp (today) at the end of the current list?" _
        & vbCrLf & vbCrLf & "Date-Stamp: " & Format(dDate, "Long Date"), vbYesNo + vbQuestion, "Confirm")
        If iResponse = vbYes Then
            
            lstListNotes.AddItem "   " & Format(dDate, "Long Date")
            lstList.AddItem "   " & Format(dDate, "Long Date")
           
            lstListSpace.AddItem "   " & Format(dDate, "Long Date")
            lstListNoteSpace.AddItem "   " & Format(dDate, "Long Date")
            miDateStamp = 2
            miFineArtsSpace = 1
         End If
    End If
    
    If lstListNotes.ListCount > 1 Then 'saves entry
    
        If miViewScreen = 0 Then
            Open "ListAT2.dat" For Output As #302
            For i = 0 To lstListNotes.ListCount - 1
            Print #302, lstListNotes.List(i)
            Next
            Close #302
            
            Open "ListAT3.dat" For Output As #303
            For i = 0 To lstList.ListCount - 1
            Print #303, lstList.List(i)
            Next
            Close #303
            
            Open "ListAT4.dat" For Output As #315
            For i = 0 To lstListSpace.ListCount - 1
            Print #315, lstListSpace.List(i)
            Next
            Close #315
            
            Open "ListAT5.dat" For Output As #318
            For i = 0 To lstListNoteSpace.ListCount - 1
            Print #318, lstListNoteSpace.List(i)
            Next
            Close #318
                 
        ElseIf miViewScreen = 1 Then
            Open "ListAT6.dat" For Output As #306
            For i = 0 To lstListNotes.ListCount - 1
            Print #306, lstListNotes.List(i)
            Next
            Close #306
            
            Open "ListAT7.dat" For Output As #307
            For i = 0 To lstList.ListCount - 1
            Print #307, lstList.List(i)
            Next
            Close #307
            
            Open "ListAT8.dat" For Output As #316
            For i = 0 To lstListSpace.ListCount - 1
            Print #316, lstListSpace.List(i)
            Next
            Close #316
            
            Open "ListAT9.dat" For Output As #319
            For i = 0 To lstListNoteSpace.ListCount - 1
            Print #319, lstListNoteSpace.List(i)
            Next
            Close #319
            
        End If
    End If

'----------
    'scrolls list boxes to last entry
    If miFontSize = 1 Then 'large type
    
        If lstList.ListCount > 13 Then
           lstList.TopIndex = lstList.ListCount - 12
        End If
        If lstListSpace.ListCount > 13 Then
            lstListSpace.TopIndex = lstListSpace.ListCount - 12
        End If
        If lstListNotes.ListCount > 13 Then
          lstListNotes.TopIndex = lstListNotes.ListCount - 12
        End If
        If lstListNoteSpace.ListCount > 13 Then
           lstListNoteSpace.TopIndex = lstListNoteSpace.ListCount - 12
        End If
    Else
    
        If lstList.ListCount > 15 Then 'small
           lstList.TopIndex = lstList.ListCount - 14
        End If
        If lstListSpace.ListCount > 15 Then
           lstListSpace.TopIndex = lstListSpace.ListCount - 14
        End If
        If lstListNotes.ListCount > 15 Then
          lstListNotes.TopIndex = lstListNotes.ListCount - 14
        End If
        If lstListNoteSpace.ListCount > 15 Then
           lstListNoteSpace.TopIndex = lstListNoteSpace.ListCount - 14
        End If
        
    End If

'----------
    Exit Sub
HandleErrors:
    Close #302
    Close #306
    Close #303
    Close #307
End Sub

Private Sub mnuArchiveOpen_Click()
    
    If cmdEditClose.Visible Then
        cmdEditClose_Click
    End If
    
    If optPlannerPage.Value = True Then
        optPlannerPage.Value = False
        optLogPage.Value = True
    End If
    
    chkLinerNotes.Value = 0  'note pad enabled, not in use
