Private Sub txtHour_Change()

    Dim iHour As Integer

    If Val(txtHour) < 12 Then
        iHour = Val(txtHour) + 1
    ElseIf Val(txtHour) = 12 Then
        iHour = 1
    Else
        iHour = 0
    End If
    
    If txtHour <> "" Then
        lblQuestionMark.Visible = False
    Else
        lblQuestionMark.Visible = True
    End If

    If Not IsNumeric(txtHour) And txtHour <> "?" And txtHour <> "" Or txtHour = "0" Then
         MsgBox "Entry must be a number from 1 to 12, indicating the hour the current Playlist is expected to start.", vbOKOnly, "Playlist Start Time Entry Error"
        txtHour = ""
        lblHour.ForeColor = &HFF0000 'blue
        txtHour.SetFocus
        Exit Sub
    End If
 
    If IsNumeric(txtHour) And Val(txtHour) > 12 Then
        MsgBox "Entry must be a number from 1 to 12, indicating the hour the current Playlist is expected to start.", vbOKOnly, Val(txtHour) & " Is Incorrect. Entry Is Limited to 12"
        txtHour = ""
        lblHour.ForeColor = &HFF0000 'blue
        txtHour.SetFocus
        Exit Sub
    End If
 '-------
 
   If mnuHourNumber.Checked = False Then
   
        If ((txtHour = "?" Or txtHour = "") And txtComposer1 = "" And txtComposition1 = "" And lstList.ListCount < 2) Then
            lblHour.Caption = "To Begin, Enter Program Start Time (hour) && Click the Tab Key"
        
        ElseIf ((txtHour = "?" Or txtHour = "") And (txtComposer1 <> "" Or txtComposition1 <> "")) Then
            lblHour.Caption = "Enter Playlist" & vbCrLf & "Start Time (Hour)"
        
        ElseIf IsNumeric(txtHour) Then
        
        
            If chkPM.Value = 0 Then
            lblHour.Caption = "Music Playlist for the" & vbCrLf & txtHour & " to " & iHour & " o'clock hour"
            ElseIf chkPM.Value = 1 Then
            
            lblHour.Caption = "Music Playlist for " & vbCrLf & txtHour & " to " & iHour & " o'clock PM"
            End If
            
        End If
        
    Else
        If mnuHourNumberProgram.Checked = False Then
            lblHour.Caption = "Playlist Hour" & vbCrLf & "Sequence Number"
        ElseIf mnuHourNumberProgram.Checked = True Then
            lblHour.Caption = "Playlist Hour Sequence" & vbCrLf & " and Program Number"
        End If
    End If
        
    If chkTracks.Value = 1 Then
        pTracksNotVisible
    End If
    
    If txtHour = "" Or txtHour = "?" Then
        chkPM.Visible = False
        chkPM.Value = 0
    Else
        chkPM.Visible = True
    End If
    '----------------
    imgImage(0).Visible = False

    If txtHour <> "" And txtHour <> "?" And IsNumeric(txtHour) Then
       imgImage(2).Visible = True
    ElseIf txtHour = "" Or txtHour = "?" Or Not IsNumeric(txtHour) Or imgImage(0).Visible = True Then
       imgImage(2).Visible = False
    End If

End Sub

